<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="3dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.8dp"
                android:color="@color/grey" />

            <solid android:color="@color/transparent"/>
        </shape>
    </item>

</layer-list>



